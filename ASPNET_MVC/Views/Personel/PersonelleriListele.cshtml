@model IEnumerable<ASPNET_MVC.Models.EntitiyFramework.Personel>

<h1>Personel Listesi</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Soyad</th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Ad</td>
                <td>@item.Soyad</td>
            </tr>
        }
    </tbody>
</table>